    },
    {
      "type": "memory",
      "key": "│ ├󰾆",
      "keyColor": "green",
    },
    {
      "type": "uptime",
      "key": "│ ├󰅐",
      "keyColor": "green",
    },
    {
      "type": "sound",
      "key": "  AUDIO",
      "format": "{2}",
